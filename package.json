  "bugs": {
    "url": "https://github.com/mmoussa2/mern-airbnb/issues"
  },
  "homepage": "https://github.com/mmoussa2/mern-airbnb#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "google-map-react": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.8.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-places-autocomplete": "^7.2.1",
